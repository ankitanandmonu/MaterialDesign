<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.materialdesignactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerActivity"></activity>
        <activity android:name=".SnackbarActivity"></activity>
        <activity android:name=".GoToTabActivity"></activity>
        <activity android:name=".CustomTabActivity"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".ScrollableTabActivity"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".TabActivity"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".TabIconActivity"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".TabIconTextactivity"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".FloatingText"
            android:parentActivityName=".GoToTabActivity"/>
        <activity android:name=".NavigationActivity"
            android:parentActivityName=".GoToTabActivity"/>

    </application>

</manifest>
